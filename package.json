      "scripts": {
            "dev": "vite",
            "build": "vite build",
            "preview": "vite preview --port 5176 --strictPort",
            "frontend:dev": "VITE_API_BASE_URL=http://localhost:4002 vite --port 5176",
            "frontend:build": "vite build",
            "frontend:preview": "vite preview --port 5176 --strictPort",
            "backend:dev": "npm --prefix backend run dev",
            "backend:start": "npm --prefix backend run start",
            "infra:up": "docker compose --env-file .env.docker -f docker-compose.yml up -d mongo redis",
            "infra:up:cloud": "docker compose --env-file .env.docker -f docker-compose.yml -f docker-compose.cloud.yml up -d redis backend frontend",
            "infra:down": "docker compose --env-file .env.docker -f docker-compose.yml stop mongo redis",
            "docker:dev:cloud": "docker compose --env-file .env.docker -f docker-compose.yml -f docker-compose.cloud.yml up -d --build",
            "docker:down": "docker compose --env-file .env.docker -f docker-compose.yml down",
            "docker:logs": "docker compose --env-file .env.docker -f docker-compose.yml logs -f",
            "dev:all": "concurrently -k -n backend,frontend -c blue,green \"npm run backend:dev\" \"npm run frontend:dev\"",
            "test": "vitest --config tools/config/vitest.config.ts run",
            "test:watch": "vitest --config tools/config/vitest.config.ts",
            "test:ui": "vitest --config tools/config/vitest.config.ts --ui"
      }
