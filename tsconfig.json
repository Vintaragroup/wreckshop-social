{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
  "@marketing-core": ["src/components/marketing-core/index.ts"],
  "@marketing-core/*": ["src/components/marketing-core/*"],
  "@marketing-site": ["apps/marketing-site/src"],
  "@marketing-site/*": ["apps/marketing-site/src/*"],
  "@marketing-site/components": ["apps/marketing-site/src/components"],
  "@marketing-site/components/*": ["apps/marketing-site/src/components/*"],
  "@marketing-site/pages": ["apps/marketing-site/src/pages"],
  "@marketing-site/pages/*": ["apps/marketing-site/src/pages/*"]
    },
    "types": ["vite/client"]
  },
  "include": ["src", "apps/marketing-site/src"]
}
